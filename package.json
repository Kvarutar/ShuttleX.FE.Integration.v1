{
  "name": "shuttlex-integration",
  "version": "0.0.8",
  "description": "ShuttleX-Integration library",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "devtools",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace shuttlex-integration-example",
    "test": "jest",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "pack": "npm pack",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "formatter": "yarn run prettier:fix && yarn run lint:fix"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/DevShuttleXInc/ShuttleX.FE.Integration.v1",
  "author": "DevShuttleXInc <dev@shuttlex.com> (https://github.com/DevShuttleXInc)",
  "license": "ShuttleX license",
  "bugs": {
    "url": "https://github.com/DevShuttleXInc/ShuttleX.FE.Integration.v1/issues"
  },
  "homepage": "https://github.com/DevShuttleXInc/ShuttleX.FE.Integration.v1#readme",
  "publishConfig": {
    "registry": "https://www.shuttlex.com/"
  },
  "dependencies": {
    "@react-native/eslint-config": "^0.72.2",
    "color-interpolate": "^1.0.5",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "react-native-slide-to-unlock": "^0.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.0",
    "@babel/plugin-proposal-optional-chaining": "^7.20.0",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/plugin-transform-shorthand-properties": "^7.23.3",
    "@babel/plugin-transform-template-literals": "^7.23.3",
    "@commitlint/config-conventional": "^17.0.2",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-community/datetimepicker": "^7.6.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/babel__core": "^7.1.14",
    "@types/jest": "^28.1.2",
    "@types/react": "~17.0.70",
    "@types/react-native": "0.72.6",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.53.0",
    "i18next": "^23.6.0",
    "jest": "^28.1.1",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-i18next": "^13.3.1",
    "react-native": "0.72.6",
    "react-native-builder-bob": "^0.20.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-shadow-2": "^7.0.8",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-redux": "^8.1.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@react-native-community/blur": "*",
    "@react-native-community/datetimepicker": "*",
    "@reduxjs/toolkit": "*",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*",
    "react-native-safe-area-context": "*",
    "react-native-shadow-2": "*",
    "react-native-svg": "*",
    "react-native-svg-transformer": "*",
    "react-redux": "*"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.0.1",
  "engines": {
    "node": ">= 18.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
